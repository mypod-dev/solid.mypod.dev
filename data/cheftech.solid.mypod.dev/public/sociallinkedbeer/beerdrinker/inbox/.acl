@prefix : <#>.
@prefix n0: <http://www.w3.org/ns/auth/acl#>.
@prefix inbox: <./>.
@prefix c: </profile/card#>.
@prefix n1: <http://xmlns.com/foaf/0.1/>.

:Owner
    a n0:Authorization;
    n0:accessTo inbox:;
    n0:agent c:me;
    n0:default inbox:;
    n0:mode n0:Control, n0:Read, n0:Write.
:Public
    a n0:Authorization;
    n0:accessTo inbox:;
    n0:agentClass n1:Agent;
    n0:default inbox:;
    n0:mode n0:Append.
